#import "XocException.h"

@interface XocStateException : XocException





/**
 Creates a exception when native state is changed to an invalid value, or when the native state is changed to an invalid value during parsing.
 
 @param fmt An NSString string formatting string
 
 @return The created XocStateException
 */



extern XocStateException *xocStateException(NSString *fmt, ...) NS_FORMAT_FUNCTION(1,2);

@end
